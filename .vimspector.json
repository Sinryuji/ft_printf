{
  "configurations": {
  "C - Launch": {
    "adapter": "CodeLLDB",
    "configuration": {
      "name": "C: Launch current file",
      "type": "lldb",
      "request": "launch",
      "externalConsole": false,
      "logging": {
        "engineLogging": true
      },
      "stopOnEntry": true,
      "stopAtEntry": true,
      "debugOptions": [],
      "MIMode": "lldb",
      "cwd": "${cwd}",
      "program": "${fileDirname}/${fileBasenameNoExtension}"
    }
  }
  }
}
